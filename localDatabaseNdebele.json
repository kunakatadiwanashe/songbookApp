[
    {
    "id": "3",
    "title": "MWARI MUCHENJERI",
    "subtitle": "Immortal, Invisible",
    "tune": "St. Denio, 569",
    "verses": [
      {
        "stanza": 1,
        "lines": [
          "Mwari Muchenjeri, hamuonekwi",
          "Makavanzwa kwatiri muchiedza,",
          "Makaropafadzwa Dombo rekare,",
          "samasimba, M’kundi, tokumbidzwa."
        ]
      },
      {
        "stanza": 2,
        "lines": [
          "Makanyararisa kunge chiedza,",
          "Hamuperi, munotonga nesimba;",
          "Ndurano yenyu, makomo marefu,",
          "Pam’soro pamakore ane rudo."
        ]
      },
      {
        "stanza": 3,
        "lines": [
          "Munopa upenyu kuna zvose;",
          "Muri mupenyu hwezvinhu zvose;",
          "Todenhera semashizha emiti;",
          "Tofa topera, asi hamusanduke."
        ]
      },
      {
        "stanza": 4,
        "lines": [
          "Baba wokubwinya, muri chiedza",
          "Ngirozi dzokudza, dzovanza zviso;",
          "Tokukudzai, tiyamurei kuona",
          "Chiedza chitsvene chinokuvanzai."
        ]
      }
    ],
    "author": "Walter Chalmers Smith (1824–1908)"
  },
  {
    "id": "4",
    "title": "MWARI NGAAKUDZWE",
    "subtitle": "To God Be The Glory",
    "tune": "To God be the glory, 576",
    "verses": [
      {
        "stanza": 1,
        "lines": [
          "Mwari ngaakudzwe nebasa rake,",
          "Norudo wakatipa mwana wake,",
          "Wakafira kudzikinura zvivi,",
          "Nokuzarura suo reupenyu.",
          "*Kudza ;She! Kudza ‘She!*",
          "*Nyika ngaimunzwe;*",
          "*Kudza ;She! Kudza ‘She!*",
          "*Vanhu ngavafare;*",
          "*Uyai kuna Baba nokuna Jesu,*",
          "*Kudzai nezvakaitirwa isu.*"
        ]
      },
      {
        "stanza": 2,
        "lines": [
          "Ruregerero rutsvene rweropa,",
          "Kumutendi chivimbiso chaMwari.",
          "Mutadzi mukuru akangotenda,",
          "Owana ruregerero rwaJesu."
        ]
      },
      {
        "stanza": 3,
        "lines": [
          "Wakatidzidzisa zvinhu zvikuru",
          "Norufaro rwedu muna Jesu, ‘She,",
          "Rufaro rwedu ruchave rukuru,",
          "Kana toona Jesu mumakore."
        ]
      }
    ],
    "author": "Fanny Crosby (1820–1915)"
  },
  {
    "id": "5",
    "title": "MWARI SAMASIMBA",
    "subtitle": "Jehovah Is Our Strength",
    "tune": "Majesty, 226; St John, 228; Millenium, 227",
    "verses": [
      {
        "stanza": 1,
        "lines": [
          "Mwari Samasimba,",
          "Iye rumbo rwedu;",
          "Naye tichakunda",
          "Vavengi vakuru.",
          "Satani anorwa nhando,",
          "Nokuti Mwari NdiMambo."
        ]
      },
      {
        "stanza": 2,
        "lines": [
          "Ishe bako redu",
          "Achazongodaro;",
          "Wakamisa isu",
          "Norwake rubato.",
          "Vavengi vanorwa nhando,",
          "Nokuti Mwari ndiMambo ."
        ]
      },
      {
        "stanza": 3,
        "lines": [
          "Jehovah Mufudzi,",
          "Anoziva zvose;",
          "Isu tiri hwai",
          "Tichafudzwa naye.",
          "Chivi norufu inhando,",
          "Nokuti Mwari ndiMambo."
        ]
      },
      {
        "stanza": 4,
        "lines": [
          "Mwari baba wedu,",
          "Tiri vana vake;",
          "Tichazongodaro,",
          "Haangatisiye.",
          "Masimba ose inhando,",
          "Nokuti Mwari ndiMambo."
        ]
      }
    ],
    "author": "attr: Samuel Barnard"
  },
 {
    "id": "14",
    "title": "UMAMBO HWENYU NGAHUUYE",
    "subtitle": "Thy Kingdom Come On Bended Knee",
    "verses": [
      {
        "stanza": 1,
        "lines": [
          "Umambo hwenyu ngahuuye!",
          "Tohunamatira;",
          "Vatendi, neshungu huru,",
          "Vanohumirira.",

          "*Kunanayira kwenguva,*",
          "*NdokwaMwari, Baba;*",
          "*Nyenyedzi dzinopupura*",
          "*Simba nenduramo.*"
        ]
      },
      {
        "stanza": 2,
        "lines": [
          "Zuva rinopenya kwazvo,",
          "Kunze kwotoyedza;",
          "Shongai hanzu, vatendi,",
          "Nguva yaswedera."
        ]
      },
      {
        "stanza": 3,
        "lines": [
          "Zuva rinopenya kwazvo,",
          "Rodzima zvitadzo;",
          "Kururama kuchamiswa",
          "Kuporese zvose."
        ]
      },
      {
        "stanza": 4,
        "lines": [
          "Ruzivo, norunyararo,",
          "Zvichafamba kwose;",
          "Zuva rake Mwari."
        ]
      }
    ],
    "author": "F.L. Hosmer (1840–1929)"
  }


























